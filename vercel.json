{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/frontend/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
